<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dtu.example.ui.CreateProjectController">
   <children>
      <TextField fx:id="projectname" layoutX="220.0" layoutY="94.0" onKeyPressed="#keyListenerCreateProject" />
      <Label fx:id="namelabel" alignment="CENTER" layoutX="230.0" layoutY="52.0" prefHeight="42.0" prefWidth="141.0" text="Pick a project name" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="278.0" layoutY="318.0" mnemonicParsing="false" onAction="#returnToViewProjects" text="Back" />
      <Button layoutX="253.0" layoutY="276.0" mnemonicParsing="false" onAction="#createProject" text="Create project" />
      <Label fx:id="projectAlreadyExistsFail" alignment="CENTER" layoutX="14.0" layoutY="153.0" prefHeight="95.0" prefWidth="573.0" />
   </children>
</AnchorPane>
