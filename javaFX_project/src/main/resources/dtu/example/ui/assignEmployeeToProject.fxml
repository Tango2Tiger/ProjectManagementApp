<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dtu.example.ui.AssignEmployeeToProjectController">
   <children>
      <ChoiceBox fx:id="projectChoiceBox" layoutX="110.0" layoutY="135.0" prefWidth="150.0" />
      <ChoiceBox fx:id="employeeChoiceBox" layoutX="353.0" layoutY="135.0" prefWidth="150.0" />
      <Button layoutX="119.0" layoutY="215.0" mnemonicParsing="false" onAction="#chooseProject" prefHeight="40.0" prefWidth="127.0" text="Choose Project">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="assignEmployeeButton" layoutX="366.0" layoutY="214.0" mnemonicParsing="false" onAction="#assignEmployee" prefHeight="42.0" prefWidth="133.0" text="Assign Employee">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="267.0" layoutY="300.0" mnemonicParsing="false" onAction="#returnToViewProjects" prefHeight="36.0" prefWidth="64.0" text="Back">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Label fx:id="assignEmployeeLabel" alignment="CENTER" layoutX="5.0" layoutY="50.0" prefHeight="52.0" prefWidth="575.0" text="Assign employee to project screen" textAlignment="CENTER">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
