<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dtu.projectmanagement.ui.LoggedInController">
    <children>
        <Label text="Logged In Screen" />
      <Button fx:id="viewEmployeesButton" mnemonicParsing="false" onAction="#viewEmployees" prefHeight="26.0" prefWidth="116.0" text="Employees" />
      <Button fx:id="projectButton" mnemonicParsing="false" onAction="#viewProjects" prefHeight="26.0" prefWidth="116.0" text="Projects" />
      <Button fx:id="activitiesButton" mnemonicParsing="false" onAction="#viewActivities" prefHeight="26.0" prefWidth="116.0" text="Activities" />
      <Button mnemonicParsing="false" onAction="#logOut" text="Log Out">
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin>
      </Button>

    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
