<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dtu.projectmanagement.ui.ViewEmployeesController">
    <children>
        <Label text="Employees">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Button fx:id="switchToRegisterEmployee" mnemonicParsing="false" onAction="#registerEmployee" text="Register Employee" />
        <Button mnemonicParsing="false" onAction="#registerTimeScreen" text="Register Time" />
      <Button fx:id="registerAbsenceButton" mnemonicParsing="false" onAction="#registerAbsence" text="Register Absence" />
        <Button fx:id="switchDeleteEMployee" mnemonicParsing="false" onAction="#deleteEmployee" text="Delete Employee" />
        <Button mnemonicParsing="false" onAction="#returnToLoggedIn" text="Back" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
